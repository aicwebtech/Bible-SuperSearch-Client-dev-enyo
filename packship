echo 'Current git branch: '
git branch | grep \*
mkdir -p ../ui-standalone/biblesupersearch
rm -r ../ui-standalone/biblesupersearch/assets

enyo pack --production -d ../ui-standalone/biblesupersearch --no-inline-css --no-inline-js
rm ../ui-standalone/biblesupersearch/config.js
cp assets/html/readme.txt ../ui-standalone/biblesupersearch/
cp assets/html/user_guide.html ../ui-standalone/biblesupersearch/
cp assets/html/prod_site_sample.html ../ui-standalone/biblesupersearch/example.html
cp assets/html/prod_test.html ../ui-standalone/biblesupersearch/test.html
cp assets/html/prod.html ../ui-standalone/biblesupersearch/index.html